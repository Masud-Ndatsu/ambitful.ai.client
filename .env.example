# Vite React Environment Variables
# These variables will be available in the React app as import.meta.env.VITE_*

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_BASE_URL=http://localhost:3000

# Environment
VITE_NODE_ENV=development

# App Configuration
VITE_APP_NAME=Destinn
VITE_APP_VERSION=1.0.0

# External Services
VITE_GOOGLE_ANALYTICS_ID=your-ga-id-here
VITE_SENTRY_DSN=your-sentry-dsn-here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT=true
VITE_ENABLE_TESTIMONIALS=true

# Social Auth (if implementing later)
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here
VITE_FACEBOOK_APP_ID=your-facebook-app-id-here

# Development
VITE_DEBUG=true
VITE_ENABLE_LOGS=true